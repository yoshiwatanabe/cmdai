{
  "CommandAI": {
    "DefaultTool": "git",
    "RequireConfirmation": true,
    "ContextAware": true,
    "SupportedTools": ["git", "az", "docker", "kubectl", "npm", "yarn"],
    "Extensions": {
      "WebCommandRepository": false,
      "AIResolver": true
    }
  },
  "AI": {
    "EnableAI": true,
    "Providers": ["azureopenai", "ollama"],
    "Provider": "ollama",
    "ModelName": "codellama:7b", 
    "OllamaEndpoint": "http://localhost:11434",
    "AzureOpenAIEndpoint": "",
    "AzureOpenAIApiKey": "",
    "AzureOpenAIModelName": "model-router",
    "TimeoutSeconds": 30,
    "FallbackToPatterns": true,
    "EnableLearning": true,
    "ConfidenceThreshold": 0.7
  }
}